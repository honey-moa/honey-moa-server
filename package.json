  "description": "honey-moa-server",
  "author": {
    "name": "hobiJeong",
    "email": "jjb26444@gmail.com",
    "url": "https://github.com/hobiJeong"
  },
  "contributors": ["BiHo Jeong <jjb26444@gmail.com>"],
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "22.12.0"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:docker": "docker build -t honey-moa-server . && docker-compose up",
    "start:db": "docker-compose up -d postgres",
    "format": "npx @biomejs/biome format --changed --write src test",
    "lint": "npx @biomejs/biome lint --changed --write src test",
    "check": "npx @biomejs/biome check --changed --write src test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate:preview": "npx prisma migrate dev --create-only --preview-feature",
    "prisma:migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.726.1",
    "@aws-sdk/client-s3": "^3.735.0",
